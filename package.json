    "@tabler/icons-react": "^2.47.0",
    "firebase": "^10.7.2",
    "lodash": "^4.17.21",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
